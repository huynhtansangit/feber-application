  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "LOCAL": "----------------------------------------------------------------------------------------------------",
    "start-l": "set PORT=1000 && npm run start",
    "POST-BUILD": "----------------------------------------------------------------------------------------------------",
    "rename-index": "node -e \"require('fs').rename('build/index.html', 'build/index.aspx', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "remove-json1": "node -e \"require('fs').unlink('build/asset-manifest.json', function(err) { if (err) console.log(err); console.log('Unneccessary json file 1 is successfully removed!') })\"",
    "remove-json2": "node -e \"require('fs').unlink('build/manifest.json', function(err) { if (err) console.log(err); console.log('Unneccessary json file 2 is successfully removed!') })\"",
    "end": "node -e \"console.log('END!')\"",
    "run-after-build": "npm run rename-index&& npm run remove-json1&&npm run remove-json2&&npm run end",
    "SANDBOX": "----------------------------------------------------------------------------------------------------",
    "build-s": "set PUBLIC_URL=http://feber.inside-apps-sandbox.bosch.com/SPA/Pages&&react-scripts build&&npm run run-after-build",
    "D2": "----------------------------------------------------------------------------------------------------",
    "build-d": "set PUBLIC_URL=https://feber.sp-apps-d2.bosch.com/SPA/Pages&&set GENERATE_SOURCEMAP=false&&react-scripts build&&npm run run-after-build",
    "D1": "----------------------------------------------------------------------------------------------------",
    "build-d1": "set PUBLIC_URL=https://feber.sp-apps-d1.bosch.com/SPA/Pages&&set GENERATE_SOURCEMAP=false&&react-scripts build&&npm run run-after-build",
    "QUALITY": "----------------------------------------------------------------------------------------------------",
    "build-q": "set PUBLIC_URL=https://feber.inside-appsq.bosch.com/SPA/Pages&&set GENERATE_SOURCEMAP=false&&react-scripts build&&npm run run-after-build",
    "PRODUCTION": "----------------------------------------------------------------------------------------------------",
    "build-p": "set PUBLIC_URL=https://feber.sp-apps.bosch.com/SPA/Pages&&set GENERATE_SOURCEMAP=false&&react-scripts build&&npm run run-after-build",
    "ONLINE": "----------------------------------------------------------------------------------------------------",
    "build-o": "set PUBLIC_URL=https://goetzinger.sharepoint.com/sites/FEBER_Dev/SPA/Pages&&set GENERATE_SOURCEMAP=false&&react-scripts build&&npm run run-after-build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
